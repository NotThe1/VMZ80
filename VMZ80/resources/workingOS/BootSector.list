0258: 0100         ;-----------------------------------------------------------------------
0259: 0100         ;; Disk block parameters for F3HD - 3.5 HD   1.44 MB Diskette
0260: 0100         ;-----------------------------------------------------------------------
0261: 0100         ;dpb3hdSPT				EQU		0090H			; cpmRecords per track- (144)
0262: 0100             dpb3hdSPT  EQU    recordsPerSector * SectorsPerTrack * NumberOfHeads ; SPT - records per Clynder
0263: 0100             dpb3hdBSH  EQU    04H                  ; Block Shift Factor - BlockSize = 128 * (2**BSH)											; BSH = Log2(BlockSize/cpmRecordSize)
0264: 0100             dpb3hdBLM  EQU    0FH                  ; BlockMask = (2**BSH) -1
0265: 0100             dpb3hdEXM  EQU    00H                  ; Extent mask = (PhysicalExtents/LogicalExtents) - 1
0266: 0100             dpb3hdDSM  EQU    DataBlocks -1        ; Maximum allocation block number (710)
