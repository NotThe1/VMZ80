0236: 0100
0237: 0100         ;----------------------3.5 Double Density Disk Geometry----------------------------------------
0238: 0100             NumberOfHeads EQU    02H                  ; number of heads
0239: 0100             TracksPerHead EQU    50H                  ; 80
0240: 0100             SectorsPerTrack EQU    12H                  ; 18 -  1 head only
0241: 0100             SectorsPerBlock EQU    04H                  ; 2048 bytes
0242: 0100             DirectoryBlockCount EQU    02H                  ;
0243: 0100         ;-----------------------------------------------------------------------
0244: 0100
0245: 0100             BlockSize  EQU    SectorsPerBlock * 	diskSectorSize ; Size in Bytes
0246: 0100
0247: 0100             RecordsPerBlock EQU    recordsPerSector * SectorsPerBlock
0248: 0100
0249: 0100             TotalNumberOfSectors EQU    SectorsPerTrack * TracksPerHead * NumberOfHeads
0250: 0100             TotalNumberOfBlocks EQU    TotalNumberOfSectors / SectorsPerBlock
0251: 0100             SectorsPerCylinder EQU    SectorsPerTrack * NumberOfHeads
0252: 0100
0253: 0100             SystemSectors EQU    LengthInBytes / diskSectorSize + 1 ; need to account for boot sector
0254: 0100             myOffset   EQU    (SystemSectors / SectorsPerCylinder) + 1 ;
0255: 0100             DataSectors EQU    TotalNumberOfSectors - (SectorsPerCylinder * myOffset)
