0300: E000         ;========================== CCP Entry ======================================;
0301: E000
0302: E000         ; 	JP		CcpStart						;start CCP with possible initial command
0303: E000         ;*****************************************************************
0304: E000         ;enter here from boot loader
0305: E000         ; On Entry	C(HiNibble) = User Number
0306: E000         ;			C(LoNibble) = Disk (0=A, 1=B...F=P)
0307: E000             CcpStart:
0308: E000 31 F0 E7               LD     SP,Stack             ; Set CCP's Stack
0309: E003 C5                     PUSH   BC                   ; Save Disk Number
0310: E004 59                     LD     E,C                  ; Get User Number
0311: E005 CB 3B                  SRL    E
0312: E007 CB 3B                  SRL    E                    ; Move user number
0313: E009 CB 3B                  SRL    E                    ;  to Lo Nibble
0314: E00B CB 3B                  SRL    E
0315: E00D CD 24 E6               CALL   SetUser              ; Set initial User
0316: E010         ; Initialize will completely reset the disk file system
0317: E010 CD E3 E5               CALL   Initialize           ; A = 0FFH if Submit file found
0318: E013 32 9D E2               LD     (submitFlag),A       ; Save return value in Flag
0319: E016 C1                     POP    BC                   ; Recall disk number
0320: E017 79                     LD     A,C                  ; Put into Acc
0321: E018 E6 0F                  AND    LO_NIBBLE_MASK       ; Remove User Number
0322: E01A 32 04 00               LD     (Pg0CurentDisk),A    ; Puts disk number into Page 0
0323: E01D CD E8 E5               CALL   SelectDisk           ; Set default Disk From Acc											; check for initial command
0324: E020 3A 1A E2               LD     A,(commandLength)
0325: E023 B7                     OR     A                    ; See if there is a Command
0326: E024 20 16                  JR     NZ,CcpParseCommand   ;  Skip if yes
0327: E026
0328: E026             ccpMainEntry:
0329: E026 31 F0 E7               LD     SP,Stack             ; (re)Establish CCP's Stack
0330: E029 CD 40 E6               CALL   PrintCrLf            ; Send CRLF to the Console
0331: E02C CD 1D E6               CALL   GetSelectedDrive     ; Get current disk number
0332: E02F C6 41                  ADD    A,ASCII_A            ; Make ASCII form of Drive
0333: E031 CD 35 E6               CALL   PrintCharInA         ; Send Drive letter to console
0334: E034 3E 3E                  LD     A,GREATER_THAN       ; Load '>'
0335: E036 CD 35 E6               CALL   PrintCharInA         ; Send to Console
0336: E039 CD 65 E0               CALL   ReadCommand          ; Command Buffer filled, Pointer set
0337: E03C
0338: E03C             CcpParseCommand:
0339: E03C 11 80 00               LD     DE,DMABuffer         ; Default in page 0
0340: E03F CD 18 E6               CALL   SetDMA               ; Set the DMA address
0341: E042 CD 1D E6               CALL   GetSelectedDrive     ; Get current drive
0342: E045 32 21 E7               LD     (currentDisk),A      ; Save
0343: E048 CD 21 E1               CALL   ParseToken1          ; Get first token in command buffer
0344: E04B C4 F5 E1               CALL   NZ,CommandError      ; The name must be unambiguous
0345: E04E 3A 1E E7               LD     A,(selectedDisk)     ; Get the Disk:
0346: E051 B7                     OR     A                    ; If not default,
0347: E052 C2 0A E5               JP     NZ,ccpUserCommand    ;   must be user CMD
0348: E055
0349: E055 CD D0 E1               CALL   IntrinsicFunction    ; Get CMD index into ACC
0350: E058 21 D6 E2               LD     HL,builtInCMDVector  ; Base of Vector
0351: E05B 5F                     LD     E,A
0352: E05C 16 00                  LD     D,0                  ; Index in DE
0353: E05E 19                     ADD    HL,DE
0354: E05F 19                     ADD    HL,DE                ; Vector is a word, need 2 ADDs
0355: E060 7E                     LD     A,(HL)               ; Pointing at Address
0356: E061 23                     INC    HL
0357: E062 66                     LD     H,(HL)
0358: E063 6F                     LD     L,A                  ; Get address to HL
0359: E064 E9                     JP     (HL)                 ;  Go to the Function
0360: E065
0361: E065
0362: E065         ;------------------------------ Read Command -------------------------------;
0363: E065         ;
0364: E065         ;read the next command into the command buffer                             ;
0365: E065         ;check for submit file                                                     ;
0366: E065             ReadCommand:                      ;
0367: E065 3A 9D E2               LD     A,(submitFlag)       ; Get the Submit File Flag          ;
0368: E068 B7                     OR     A                    ; Is it Set ?                       ;
0369: E069 28 54                  JR     Z,NotSubmitFile      ;  skip if not                      ;
0370: E06B         ; scanning a submit file change drives to open and read the file            ;
0371: E06B 3A 21 E7               LD     A,(currentDisk)      ; Get current disk                  ;
0372: E06E B7                     OR     A                    ; Is it default ?                   ;
0373: E06F 3E 00                  LD     A,0                  ; Prepare to set Disk A             ;
0374: E071 C4 E8 E5               CALL   NZ,SelectDisk        ; Select it if needed               ;
0375: E074         ; have to open again in case xsub present                                   ;
0376: E074 11 9E E2               LD     DE,submitFCB         ; Point at the Submits's FCB        ;
0377: E077 CD EE E5               CALL   OpenFile             ; Open it                           ;
0378: E07A 28 43                  JR     Z,NotSubmitFile      ; Skip if no submit file present    ;
0379: E07C 3A AC E2               LD     A,(submitRC)         ; Get Number of records in file     ;
0380: E07F 3D                     DEC    A                    ; read last record(s) first     ;
0381: E080 32 BD E2               LD     (submitCR),A         ; current record to read            ;
0382: E083 11 9E E2               LD     DE,submitFCB         ; Point at the Submits's FCB        ;
0383: E086 CD F8 E5               CALL   DiskRead             ; Read from back to front           ;
0384: E089 20 34                  JR     NZ,NotSubmitFile     ; Skip EOF ??                       ;
0385: E08B         ;
0386: E08B         ; disk read is OK, transfer to commandBuffer                                ;
0387: E08B 21 80 00               LD     HL,DMABuffer         ; From                              ;
0388: E08E 11 1A E2               LD     DE,commandLength     ; TO	                            ;
0389: E091 01 80 00               LD     BC,cpmRecordSize     ; For this many bytes               ;
0390: E094 ED B0                  LDIR                        ; Move to command buffer            ;
0391: E096         ;
0392: E096         ; line is transferred, close the file with a deleted record                 ;
0393: E096 21 AB E2               LD     HL,submitFWF         ; Point at the WriteFileFlag        ;
0394: E099 CB BE                  RES    7,(HL)               ; Clear the WriteFileFlag                   ;
0395: E09B         ;	LD		HL,submitFWF				; Bit 7 is FileWriteFlag            ;
0396: E09B         ;	LD		(HL),0						                                    ;
0397: E09B         ;	INC		HL                                                              ;
0398: E09B 21 AC E2               LD     HL,submitRC          ; Point at FCB record Count         ;
0399: E09E 35                     DEC    M                    ; Make it one less record           ;
0400: E09F 11 9E E2               LD     DE,submitFCB         ; Point at the Submits's FCB        ;
0401: E0A2 CD F3 E5               CALL   CloseFile            ; Close it                          ;
0402: E0A5 28 18                  JR     Z,NotSubmitFile      ; Skip if not clean close           ;
0403: E0A7         ; CloseFile went ok, return to original drive                               ;
0404: E0A7 3A 21 E7               LD     A,(currentDisk)      ; Get the current disk              ;
0405: E0AA B7                     OR     A                    ; Is it the default disk ?          ;
0406: E0AB C4 E8 E5               CALL   NZ,SelectDisk        ;  no, then go select it            ;
0407: E0AE         ; print to the 00                                                           ;
0408: E0AE 21 1B E2               LD     HL,commandBuffer     ; Point at the command buffer       ;
0409: E0B1 CD 56 E6               CALL   PrintStringNull      ; Display it on the Console         ;
0410: E0B4 CD 6E E6               CALL   CheckForConsoleChar  ; User input a character ?          ;
0411: E0B7 28 17                  JR     Z,NoRead             ;  skip if not                      ;
0412: E0B9 CD EE E0               CALL   DeleteSubmitFile     ; Remove file and reset submit flag ;
0413: E0BC C3 26 E0               JP     ccpMainEntry         ; Start a new CCP session           ;
