0290: E000
0291: E000             fcbSystemFileIndex EQU    0AH                  ; extent number field index
0292: E000             FILE_NAME_SIZE EQU    08H                  ; Max size of file Name
0293: E000             FILE_TYPE_SIZE EQU    03H                  ; Max size of file Type
0294: E000
